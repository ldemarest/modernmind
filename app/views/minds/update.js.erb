


function changeUpvote() { 
  

  $('#upvote-count').html("Upvotes: "+'<%= j @upvotes.count.to_s %>')

  $('#upvote-button-complete').val() == "Upvote" ? upvote_Start() : upvoted();


  }

function upvote_Start() {
    $('#upvote-button-complete').val("Upvoted");


  }
  
function upvoted() {
    $('#upvote-button-complete').val("Upvote");

  }


$('#upvote-button').on('click', changeUpvote());





