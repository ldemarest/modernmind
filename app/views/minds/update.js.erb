
function changeUpvote() { 
  

  $('#upvote-count').html("Upvotes: "+'<%= j @upvotes.count.to_s %>')

  $('#upvote-button').val() == "Upvote" ? upvote_Start() : upvoted();


  }

function upvote_Start() {
    $('#upvote-button').val("Upvoted");


  }
  
function upvoted() {
    $('#upvote-button').val("Upvote");

  }


$('#upvote-button').live('click', changeUpvote());





